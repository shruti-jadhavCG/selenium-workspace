package ddtPrograms;

import java.io.FileInputStream;

import org.apache.poi.ss.usermodel.Workbook;
import org.apache.poi.ss.usermodel.WorkbookFactory;
import org.apache.poi.ss.util.NumberToTextConverter;

public class NumToText 
{

	FileInputStream f=new FileInputStream("C:\\Users\\Shruti\\selenium-workspace\\seleniumBasics\\TestData\\shruti.xlsx");
	Workbook wb = WorkbookFactory.create(f);

//	String mobileno= wb.getSheet("Sheet1").getRow(1).getCell(2).getStringCellValue();
	String mobileno= NumberToTextConverter.toText(wb.getSheet("Sheet1").getRow(1).getCell(2).getNumericCellValue());
}
